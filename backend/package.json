{
  "name": "geospatial-admin-backend",
  "version": "1.0.0",
  "description": "Geospatial Administration System Backend",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/server.ts",
    "start": "node dist/server.js",
    "build": "tsc",
    "lint": "eslint src/**/*.{ts,js} --fix",
    "lint:check": "eslint src/**/*.{ts,js}",
    "db:populate": "ts-node -r tsconfig-paths/register src/scripts/populate.ts",
    "db:clear": "ts-node -r tsconfig-paths/register src/scripts/clear.ts",
    "db:geojeson": "ts-node -r tsconfig-paths/register src/scripts/populateQuadrants.ts"
  },
  "keywords": [
    "geospatial",
    "administration",
    "backend",
    "api",
    "typescript",
    "express",
    "sequelize",
    "leaflet",
    "postgis"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tmcw/togeojson": "^7.1.2",
    "@types/validator": "^13.15.2",
    "@types/xmldom": "^0.1.34",
    "@xmldom/xmldom": "^0.8.11",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.35.0",
    "sequelize-typescript": "^2.1.5",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.8.0",
    "eslint": "^9",
    "eslint-config-prettier": "^9",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "globals": "^14.0.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2",
    "typescript-eslint": "^7"
  },
  "resolutions": {
    "@types/express": "4.17.17"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
